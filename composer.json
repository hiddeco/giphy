{
    "name": "hiddeco/giphy",
    "type": "library",
    "description": "Simple PHP Giphy API client",
    "keywords": [
        "hiddeco",
        "giphy",
        "php",
        "api",
        "client",
        "wrapper"
    ],
    "homepage": "https://github.com/hiddeco/giphy",
    "license": "MIT",
    "authors": [
        {
            "name": "Hidde Beydals",
            "email": "hello@hidde.co",
            "homepage": "https://github.com/hiddeco"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^0.8.0",
        "php-http/message-factory": "^1.0",
        "php-http/message": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "php-http/guzzle6-adapter": "^1.0",
        "puli/composer-plugin": "^1.0@beta"
    },
    "autoload": {
        "psr-4": {
            "Giphy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Giphy\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
